	function submitRetriveGroupId()
	{
		
		var currentElement = document.createElement("input");
		currentElement.setAttribute("type", "hidden");
		currentElement.setAttribute("name", "_eventId_siteHelp");
		currentElement.setAttribute("id", "_eventId_siteHelp");
		currentElement.setAttribute("value", "_eventId_siteHelp");
		var headdiv=document.getElementById('link');
		headdiv.appendChild(currentElement);
		document.forms[0].submit();
	
	}
